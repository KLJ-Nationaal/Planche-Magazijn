  "version": "0.2.0",
  "configurations": [
    {
      "name": "Angular: Launch Vivaldi",
      "type": "pwa-chrome",
      "request": "launch",
      "url": "http://localhost:4200",
      "webRoot": "${workspaceFolder}",
      "runtimeExecutable": "C:\\Users\\kevin\\AppData\\Local\\Vivaldi\\Application\\vivaldi.exe",
      "presentation": { "reveal": "always" },
      "sourceMapPathOverrides": {
        "webpack:///src/*": "${webRoot}/src/*",
        "webpack:///*": "*",
        "webpack:///./*": "${webRoot}/*"
      }
    }
  ]
}
